{
  "name": "@ltv/moleculer-apollo-server-mixin",
  "version": "0.1.27",
  "description": "An Apollo Server v4 mixin for moleculerjs. Schema first & compatible with GraphQL codegen",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "prettier:write": "prettier --config .prettierrc --ignore-path=.prettierignore --write **/*.ts",
    "prebuild": "rimraf dist",
    "build": "node .esbuild.js",
    "init:husky": "husky install",
    "postbuild": "tsc -p tsconfig.json --emitDeclarationOnly --outDir dist",
    "prepush": "npm run build",
    "init:cz": "commitizen init cz-conventional-changelog --yarn --dev --exact",
    "latest-publish": "npm publish",
    "latest-prepare": "standard-version --no-verify"
  },
  "keywords": [
    "moleculer",
    "moleculer-mixin",
    "moleculer-apollo-server",
    "moleculer-graphql",
    "moleculer-graphql-server",
    "moleculer-graphql-mixin",
    "moleculer-apollo-server-mixin",
    "apollo-server"
  ],
  "author": "Luc <luc@ltv.vn>",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@apollo/subgraph": "^2.3.0",
    "graphql": "^16.6.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.omit": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jest": "^29.4.0",
    "@types/lodash.defaultsdeep": "^4.6.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/node": "^18.11.18",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.5",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "moleculer": "^0.14.28",
    "moleculer-web": "^0.10.5",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
